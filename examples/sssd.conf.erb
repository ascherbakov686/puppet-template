[sssd]
debug_level = 3
domains = domain.ru,domain.com,us.domain.com,apac.domain.com,de.domain.com,eu.domain.com,fr.domain.com,uk.domain.com
domain_resolution_order = us.domain.com,apac.domain.com,de.domain.com,eu.domain.com,fr.domain.com,uk.domain.com,domain.ru,domain.com
config_file_version = 2
services = nss
reconnection_retries = 10
sbus_timeout = 15
client_idle_timeout = 86400
fd_limit = 65536
timeout = 15

[nss]
debug_level = 3
filter_users = impala,root,hive,nobody
enum_cache_timeout = 2400
entry_cache_nowait_percentage = 95
memcache_timeout = 3600
reconnection_retries = 10
client_idle_timeout = 86400
responder_idle_timeout = 86400
fd_limit = 65536
timeout = 15
#cache_first = true

[domain/domain.ru]
full_name_format = %1$s
override_homedir = /home/%u
ldap_user_search_base = DC=avp,DC=ru?sub?(|(memberOf=CN=prj-team,OU=Groups,OU=Clients,DC=domain,DC=ru))
selinux_provider = none
subdomain_enumerate = domain.ru,domain.com,us.domain.com,apac.domain.com,de.domain.com,eu.domain.com,fr.domain.com,uk.domain.com
filter_users = impala,root,hive,nobody
client_idle_timeout = 86400
#ad_enable_gc = false
ldap_idmap_default_domain_sid = S-1-5-21-1430328663-2098613005-1233803906
#ldap_use_tokengroups = False
ad_enabled_domains = domain.ru,domain.com,us.domain.com,apac.domain.com,de.domain.com,eu.domain.com,fr.domain.com,uk.domain.com
dyndns_update = false
dyndns_update_ptr = false
debug_level = 3
ldap_network_timeout = 15
ldap_sasl_authid = <%= @hostname.upcase %>$@domain.ru
ad_server = dc1.domain.ru,dc2.domain.ru,dc3.domain.ru,dc4.domain.ru,dc5.domain.ru,dc6.domain.ru,dc7.domain.ru,dc8.domain.ru,dc9.domain.ru
ad_domain = domain.ru
#krb5_realm = domain.ru
#realmd_tags = manages-system joined-with-adcli
id_provider = ad
ldap_id_mapping = true
use_fully_qualified_names = False
#entry_cache_timeout = 3000
#refresh_expired_interval = 1200
ad_hostname = <%= @hostname.upcase %>$@domain.ru
ignore_group_members = True
ldap_purge_cache_timeout = 0

